<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
    <body>
        <h1>Here Are Some Delicious Foods!</h1>
        @{
            var StringList = new List <string>()
                {
                    "Apple Pies",
                    "Burritos",
                    "Clams Casino",
                    "Donuts",
                };
            foreach(var word in StringList)
            {
                <p>@word</p>
            }
        }
    </body>
</html>